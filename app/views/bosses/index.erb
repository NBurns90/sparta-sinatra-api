<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h1 class="center">Boss Index</h1>
    </div>
  </div>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Description</th>
      </tr>
    </thead>
    <tbody>
      <% @multiboss.first.each do |boss| %>
      <tr>
        <td><a href="/boss/<%= boss['id'] %>"</a><%= boss['id'] %></td>
        <td><%= boss['name'] %></td>
        <td><%= boss['description'] %></td>
      </tr>
      <% end  %>
    </tbody>
  </table>
</div>
